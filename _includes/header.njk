<header>
  <nav class="nav">
    <h4 class="nav__title">
      <a href="/home"> <img src="/static/icons/kaex.png" style="max-width: 180px;"/>  </a>
      <o> / {{title}} </o>
    </h4>
    <ul class="nav__links">
      {% for link in collections.nav %}
        <li{% if link.url == post.url %} aria-current="page"{% endif %} class="sidebar__link">
          <a href={{link.url}}>
            {{link.data.title}}
          </a>
        </li>
      {% endfor %}
      <li{% if link.url == post.url %} aria-current="page"{% endif %} class="sidebar__link"><a href="/pages/page-list">Meta</a></li>
    </ul>
  </nav>
  <nav class="taglist">
    {% for tag in collections.tagList %}
      {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
    {% endfor %}
  </nav>
</header>